<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="603.0" prefWidth="897.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="JXML_Game.Main_pageController">
   <children>
      <ImageView fx:id="backGround" fitHeight="604.0" fitWidth="911.0" layoutX="-7.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/BackGround.jpg" />
         </image>
      </ImageView>
      <Button fx:id="sound_button" layoutX="830.0" layoutY="21.0" mnemonicParsing="false" onAction="#soundOpreater" onMousePressed="#sound_play" prefHeight="46.0" prefWidth="46.0" style="-fx-background-radius: 100; -fx-background-image: Images\audio.png;">
         <graphic>
            <ImageView fx:id="image_sound" fitHeight="42.0" fitWidth="79.0" onMousePressed="#sound_play" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Images/audio.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="play_button" layoutX="830.0" layoutY="537.0" mnemonicParsing="false" onAction="#moveToScene2" prefHeight="52.0" prefWidth="62.0" style="-fx-background-radius: 100;">
         <graphic>
            <ImageView fx:id="play_image" fitHeight="50.0" fitWidth="41.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Images/play.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
